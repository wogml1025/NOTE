{
  "manifest_version": 2,

  "name": "mynoteCapture",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.0",

  "browser_action": {
    "default_icon": "/public/images/note.png",
    "default_popup": "/views/popup.ejs"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "<all_urls>",
    "storage",
    "http://34.209.115.97:3000/*"
  ]
}
